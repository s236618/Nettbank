@model List<Nettbank.Model.User>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserList</title>
</head>
<body>
    <br /><br />
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].persNr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].adress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].zip)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.persNr)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.adress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.zip)
                </td>
                <td>
                    @Html.ActionLink("Endre", "Change", new { id = item.persNr }, new { @class = "btn btn-success" })
                    @Html.ActionLink("Detalj", "Details", new { id = item.persNr }, new { @class = "btn btn-primary" })
                    @Html.ActionLink("Slette", "Delete", new { id = item.persNr }, new { @class = "btn btn-danger" })
                </td>
            </tr>
        }

    </table>
    @Html.ActionLink("Ny bruker", "Register", new { tom = "" }, new { @class = "btn btn-success" })
</body>
</html>
